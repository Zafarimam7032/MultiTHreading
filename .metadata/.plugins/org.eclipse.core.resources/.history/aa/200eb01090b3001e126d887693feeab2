public class MovingTotal {

	public static int display() {
		try {
			throw new Exception();
		}catch (Exception e) {
			// TODO: handle exception
			return 2;

		}finally {
			return 3;

		}
		
	}
	public static void main(String[] args) {
		int display = display();
		System.out.println(display);
	}
}